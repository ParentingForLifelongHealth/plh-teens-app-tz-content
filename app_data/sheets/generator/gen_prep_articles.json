{
  "flow_type": "generator",
  "flow_name": "gen_prep_articles",
  "status": "released",
  "parameter_list": {
    "input_data_list": "modules",
    "output_flow_name": "prep_{@gen.id}_articles",
    "output_flow_type": "data_pipe",
    "output_flow_subtype": "generated"
  },
  "rows": [
    {
      "operation": "filter",
      "args_list": [
        "id == \"@gen.relax_id\""
      ],
      "input_source": "relax"
    },
    {
      "operation": "append_columns",
      "args_list": [
        "category_id: relax",
        "nested: FALSE"
      ],
      "output_target": "{@gen.id}_relax | local_only;"
    },
    {
      "operation": "filter",
      "args_list": [
        "id == \"@gen.check_in_id\""
      ],
      "input_source": "check_in"
    },
    {
      "operation": "append_columns",
      "args_list": [
        "category_id: check_in",
        "nested: FALSE"
      ],
      "output_target": "{@gen.id}_check_in | local_only;"
    },
    {
      "operation": "filter",
      "args_list": [
        "!nested"
      ],
      "input_source": "{@gen.id}_articles",
      "output_target": "{@gen.id}_testing | local_only;"
    },
    {
      "operation": "concat",
      "args_list": [
        "{@gen.id}_relax",
        "{@gen.id}_check_in",
        "{@gen.id}_testing"
      ],
      "input_source": false
    },
    {
      "operation": "append_columns",
      "args_list": [
        "completed: FALSE"
      ],
      "output_target": "{@gen.id}_article_tasks"
    }
  ],
